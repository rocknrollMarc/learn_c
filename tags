Database_list	ex17.c	/^void Database_list(struct Connection *conn)$/;"	f
Database_load	ex17.c	/^void Database_load(struct Connection *conn)$/;"	f
Database_open	ex17.c	/^struct Connection *Database_open(const char *filename, char mode)$/;"	f
Database_set	ex17.c	/^void Database_set(struct Connection *conn, int id, const char *name, const char *email)$/;"	f
Database_write	ex17.c	/^void Database_write(struct Connection *conn)$/;"	f
MAX_DATA	ex17.c	/^#define MAX_DATA /;"	d	file:
MAX_ROWS	ex17.c	/^#define MAX_ROWS /;"	d	file:
can_print_it	ex14.c	/^int can_print_it(char ch)$/;"	f
db	ex17.c	/^    struct Database *db;$/;"	m	struct:Connection	typeref:struct:Connection::Database	file:
die	ex17.c	/^void die(const char *message)$/;"	f
email	ex17.c	/^    char email[MAX_DATA];$/;"	m	struct:Address	file:
file	ex17.c	/^    FILE *file;$/;"	m	struct:Connection	file:
id	ex17.c	/^    int id;$/;"	m	struct:Address	file:
